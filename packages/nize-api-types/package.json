{
  "name": "@six5536/nize-api-types",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./generated": {
      "import": "./src/generated/api.d.ts",
      "types": "./src/generated/api.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "generate": "openapi-typescript ../../codegen/nize-api/tsp-output/openapi.json -o src/generated/api.d.ts"
  },
  "devDependencies": {
    "openapi-typescript": "^7.10.1",
    "typescript": "^5.8.0"
  }
}
